{
  "name": "triangular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run watch",
    "build": "ng build",
    "test": "ng test",
    "build:triangular-engine": "ng build triangular-engine",
    "publish": "npm run build:triangular-engine && cd dist/triangular-engine && npm publish",
    "link": "ng build triangular-engine --configuration development && cd dist/triangular-engine && npm link",
    "watch": "ng build triangular-engine --watch --configuration development",
    "local": "npm run link && npm run watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@dimforge/rapier3d-compat": "^0.14.0",
    "dexie": "^4.0.11",
    "rxjs": "~7.8.0",
    "three": "^0.173.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/three": "^0.173.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.2"
  }
}
